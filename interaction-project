    <title>p5js Template</title>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.5.3/p5.js"></script>
    
    <script>
    var scale
        function setup() {
            createCanvas(800,600);
        }

        function draw() {
            //wings
            ellipse(mouseX-22,mouseY-100,35,70)
            ellipse(mouseX+18,mouseY-100,35,70)
            
            //body
            ellipse(mouseX,mouseY,160)
            ellipse(mouseX,mouseY,130)
            ellipse(mouseX,mouseY,100)
            
            //eyes
            ellipse(mouseX-25,mouseY-20,40)
            ellipse(mouseX+25,mouseY-20,40)
            ellipse(mouseX-20,mouseY-25,10)
            ellipse(mouseX+30,mouseY-25,10)
        }
        
        function keyPressed(){
            background(random(255),random(255),random(255))
        }
        
        function mousePressed(){
        fill(random(255),random(255),random(255))
        }
    </script>
</head>

